    "version": "0.2.0",
    "configurations": [
        {
            "name": "bolt_engine",
            "request": "launch",
            "type": "dart",
            "program": "bin/bolt_engine.dart"
        },
        {
            "name": "Debug Native",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/linux/x64/debug/bundle/app",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/linux/x64/debug/bundle",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
